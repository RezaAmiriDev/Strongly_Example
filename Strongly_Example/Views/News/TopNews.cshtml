@model IEnumerable<ClassLibrary.Page>


<section class="last-news-side border-radius">
    <header><i class="icons icon-star"></i> <h3> پربازدیدترین اخبار </h3></header>
    <ul>
      @foreach(var item in Model)
        {
            <li>
                <img class="border-radius" src="/PageImage/@item.ImageName">
                <h2> <a href="/News/@item.PageId">@item.Title</a></h2>
                <span>انتشار : @item.CreateDate</span>
            </li>
        }
    </ul>
</section>